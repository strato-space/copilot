{
  "name": "admin_app",
  "version": "1.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json && vite build",
    "build-dev": "tsc -p tsconfig.json && vite build --mode development",
    "build-local": "tsc -p tsconfig.json && vite build --mode localhost",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest --maxWorkers=${JEST_MAX_WORKERS:-50%}",
    "test:serial": "jest --runInBand",
    "e2e:install": "playwright install --with-deps chromium",
    "test:e2e": "playwright test",
    "test:e2e:non-voice": "playwright test e2e/auth.spec.ts e2e/crm-kanban.spec.ts e2e/operops.spec.ts e2e/task-create.spec.ts",
    "test:e2e:shard:1of2": "playwright test e2e/auth.spec.ts e2e/crm-kanban.spec.ts e2e/operops.spec.ts e2e/task-create.spec.ts --shard=1/2",
    "test:e2e:shard:2of2": "playwright test e2e/auth.spec.ts e2e/crm-kanban.spec.ts e2e/operops.spec.ts e2e/task-create.spec.ts --shard=2/2",
    "test:e2e:voice:shard:1of2": "playwright test e2e/voice.spec.ts e2e/voice-fab-lifecycle.spec.ts e2e/voice-log.spec.ts --project=chromium-unauth --shard=1/2",
    "test:e2e:voice:shard:2of2": "playwright test e2e/voice.spec.ts e2e/voice-fab-lifecycle.spec.ts e2e/voice-log.spec.ts --project=chromium-unauth --shard=2/2",
    "test:e2e:unauth": "playwright test --project=chromium-unauth",
    "test:e2e:auth": "playwright test --project=setup --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@types/classnames": "^2.3.0",
    "antd": "^6.2.1",
    "antd-mask-input": "^2.0.7",
    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.23",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-drag-listview": "^2.0.0",
    "react-quill-new": "^3.8.3",
    "react-router-dom": "^7.12.0",
    "sanitize-html": "^2.17.0",
    "socket.io-client": "^4.8.3",
    "universal-cookie": "^8.0.1",
    "uuid": "^13.0.0",
    "victory": "^37.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.0",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
